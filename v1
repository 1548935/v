#include<stdio.h>
#include<string.h>
int main()
{
  char a[]="1+2+2+1+2+6+4-1-1+4-8";
  int s=a[0]-'0';
  for(int i=1;i<strlen(a);i++)
  {
    if(a[i]=='+')
    {
      int rigthopreand=a[i+1]-'0';
      s+=rightoperand;
      i++;
    }
    else if(a[i]=='-')
    {
      int rigthopreand=a[i+1]-'0';
      s+=rightoperand;
      i++;
    }
  }
  printf("s=%d",s);
  return 0;
}
